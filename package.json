{
  "name": "astro-starter",
  "type": "module",
  "version": "0.0.1",
  "description": "A modern, SEO-optimized Astro starter template with comprehensive meta tags, social media integration, and essential web development tools.",
  "author": "Ali Piry",
  "license": "MIT",
  "keywords": [
    "astro",
    "starter",
    "template",
    "seo",
    "web-development",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alipiry/astro-starter.git"
  },
  "bugs": {
    "url": "https://github.com/alipiry/astro-starter/issues"
  },
  "homepage": "https://github.com/alipiry/astro-starter#readme",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "clean": "rimraf dist .astro",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --ext .js,.ts,.astro",
    "lint:fix": "eslint . --ext .js,.ts,.astro --fix",
